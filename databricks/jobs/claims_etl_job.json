{
  "name": "claims-etl-job",
  "email_notifications": {
    "on_start": [],
    "on_success": [],
    "on_failure": []
  },
  "timeout_seconds": 7200,
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "bronze_ingest",
      "notebook_task": {"notebook_path": "/Repos/you/01_bronze_ingest"},
      "new_cluster": {
        "spark_version": "13.3.x-scala2.12",
        "node_type_id": "i3.xlarge",
        "num_workers": 2
      }
    },
    {
      "task_key": "silver_transform",
      "depends_on": [{"task_key": "bronze_ingest"}],
      "notebook_task": {"notebook_path": "/Repos/you/02_silver_transform_scD2_mask"},
      "existing_cluster_id": "REPLACE_WITH_CLUSTER_ID"
    },
    {
      "task_key": "gold_analytics",
      "depends_on": [{"task_key": "silver_transform"}],
      "notebook_task": {"notebook_path": "/Repos/you/03_gold_analytics"},
      "existing_cluster_id": "REPLACE_WITH_CLUSTER_ID"
    },
    {
      "task_key": "optimizations",
      "depends_on": [{"task_key": "gold_analytics"}],
      "notebook_task": {"notebook_path": "/Repos/you/04_optimizations"},
      "existing_cluster_id": "REPLACE_WITH_CLUSTER_ID"
    }
  ],
  "format": "MULTI_TASK"
}
